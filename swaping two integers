void swap (int& a, int& b)
{
    int temp;
    a = a + b;
    b = a - b;
    a = a - b;
    
}


/*
Another way is to write a function like this:

void swap (int& a, int& b)
{
    int temp;
    temp = a;
    a = b;
    b = temp;
}

*/



int main()
{
    int five = 5;
    int four = 4;


    cout << "before swap, five:" << five << ", four:" << four << endl;

    swap(five, four);

    cout << "after swap, five:" << five << ", four:"<< four;


}
